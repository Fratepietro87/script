  set jj = \`grep \${VERS_FRM} ${CCM45_DBlog}/FRM.tmp | awk '{print substr(\$1,1,5)}'\`

  if ( \${VERS_FRM} != \$jj )  then
    echo \${VERS_FRM}:  \${VERS_FRM} > ${CCM45_DBlog}/FRM.tmp
    ccm set text_editor "cat ${CCM45_DBlog}/FRM.tmp >> %filename"
    ccm query -t admin -n base
